    "h, m = input().split()\n",
    "plus = input()\n",
    "\n",
    "h = int(h)\n",
    "m = int(m)\n",
    "plus = int(plus)\n",
    "\n",
    "h += plus // 60\n",
    "m += plus % 60\n",
    "\n",
    "if m >= 60:\n",
    "    h += 1\n",
    "    m -= 60\n",
    "    \n",
    "if h >= 24:\n",
    "    h -= 24\n",
    "    \n",
    "print(h, m)"
   ]

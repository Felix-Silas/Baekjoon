    "price = int(input())\n",
    "T = int(input())\n",
    "total = 0\n",
    "\n",
    "for i in range(T):\n",
    "    price_t, num_t = map(int, input().split())\n",
    "    total += (price_t * num_t)\n",
    "\n",
    "if total == price:\n",
    "    print('Yes')\n",
    "else:\n",
    "    print('No')"
